{
    "pipeline": {
        "input": [
            "img"
        ],
        "output": [
            "post_output"
        ],
        "tasks": [
            {
                "type": "Task",
                "module": "Transform",
                "name": "Preprocess",
                "input": [
                    "img"
                ],
                "output": [
                    "prep_output"
                ],
                "transforms": [
                    {
                        "type": "LoadImageFromFile",
                        "color_type": "color_ignore_orientation"
                    },
                    {
                        "type": "Resize",
                        "img_scale": [
                            2944,
                            736
                        ],
                        "keep_ratio": true,
                        "size": [
                            736,
                            1333
                        ]
                    },
                    {
                        "type": "Normalize",
                        "mean": [
                            123.675,
                            116.28,
                            103.53
                        ],
                        "std": [
                            58.395,
                            57.12,
                            57.375
                        ],
                        "to_rgb": true
                    },
                    {
                        "type": "Pad",
                        "size_divisor": 32
                    },
                    {
                        "type": "ImageToTensor",
                        "keys": [
                            "img"
                        ]
                    },
                    {
                        "type": "Collect",
                        "keys": [
                            "img"
                        ],
                        "meta_keys": [
                            "filename",
                            "img_norm_cfg",
                            "valid_ratio",
                            "flip_direction",
                            "ori_filename",
                            "ori_shape",
                            "scale_factor",
                            "pad_shape",
                            "img_shape",
                            "flip"
                        ]
                    }
                ]
            },
            {
                "name": "dbnet",
                "type": "Task",
                "module": "Net",
                "input": [
                    "prep_output"
                ],
                "output": [
                    "infer_output"
                ],
                "input_map": {
                    "img": "input"
                }
            },
            {
                "type": "Task",
                "module": "mmocr",
                "name": "postprocess",
                "component": "DBHead",
                "params": {
                    "text_repr_type": "quad",
                    "in_channels": 256,
                    "loss": {
                        "type": "DBLoss",
                        "alpha": 5.0,
                        "beta": 10.0,
                        "bbce_loss": true
                    }
                },
                "output": [
                    "post_output"
                ],
                "input": [
                    "prep_output",
                    "infer_output"
                ]
            }
        ]
    }
}